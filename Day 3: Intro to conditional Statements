#include <bits/stdc++.h>

using namespace std;



int main()
{
    int N;
    cin >> N;
    cin.ignore(numeric_limits<streamsize>::max(), '\n');
    if (N%2!=0){//check if even
        cout << "Weird";
    }
    else if ((N%2 == 0) && (N<=5) && (N>=2)) {
        cout<< "Not Weird";
    }
    else if ((N%2 == 0) && (N<=20) && (N>=6)){
        cout<< "Weird";
    }
    else if ((N%2 == 0) && (N>20)){
        cout << "Not Weird";
    }
    



}

   
